@model Pathology.Models.RegisterPatient

@{
    ViewData["Title"] = "Details";
}

<h1>Registered Patient Details</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Patient ID
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Patient.PatientID)
        </dd>
        <dt class = "col-sm-2">
            Test Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TestMgmt.TestName)
        </dd>
        <dt class="col-sm-2">
            Package Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Package.PackageName)
        </dd>
        <dt class="col-sm-2">
            Register DateTime
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RegDateTime)
        </dd>
        <dt class = "col-sm-2">
            Total Amount
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TotalAmount)
        </dd>
        <dt class = "col-sm-2">
            Registered By
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.Email)
        </dd>
        <dt class = "col-sm-2">
            Is Report Generated
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IsReportGenerated)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.RegisterID" class="btn btn-primary">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>

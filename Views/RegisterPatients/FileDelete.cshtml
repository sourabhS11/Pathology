@model Pathology.ViewModels.UploadFile;
@{
    ViewData["Title"] = "FileDelete";
}

<h1>Delete File</h1>

<h3>Are you sure you want to delete this File?</h3>
<form method="post" asp-action="DeletePdf">
    <div>
        <input type="hidden" readonly asp-for="Id" value=@ViewBag.Id />
        <button type="submit" class="btn btn-danger">Delete</button>
    </div>
</form>

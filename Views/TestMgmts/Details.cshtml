@model Pathology.Models.TestMgmt

@{
    ViewData["Title"] = "Details";
}

<h1>Test Details</h1>
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            TestName
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TestName)
        </dd>
        <dt class="col-sm-2">
            TestPrice
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TestPrice)
        </dd>
        <dt class="col-sm-2">
            Test Category
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TestCategory.TestCategoryName)
        </dd>
        <dt class="col-sm-2">
            Package 
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Package.PackageName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.TestId" class="btn btn-primary">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
